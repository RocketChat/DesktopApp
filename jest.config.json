{
  "projects": [
    {
      "runner": "@jest-runner/electron",
      "testEnvironment": "@jest-runner/electron/environment",
      "testMatch": ["<rootDir>/src/!(main)/**/*.(spec|test).{js,ts,tsx}"]
    },
    {
      "runner": "@jest-runner/electron/main",
      "testEnvironment": "node",
      "testMatch": ["<rootDir>/src/main/**/*.(spec|test).{js,ts,tsx}"]
    }
  ]
}
